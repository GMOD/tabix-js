    "@typescript-eslint/parser": "^2.3.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "documentation": "^9.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "npm-watch": "^0.5.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "standard-changelog": "^1.0.0",
    "typescript": "^3.6.3"
  },
