{
  "name": "@gmod/tabix",
  "version": "1.5.3",
  "description": "Read Tabix-indexed files, supports both .tbi and .csi indexes",
  "license": "MIT",
  "repository": "GMOD/tabix-js",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint": "eslint --ext .js,.ts src test",
    "clean": "rimraf dist esm",
    "prebuild": "npm run clean && npm run lint",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:es5": "tsc --target es5 --outDir dist",
    "build": "npm run build:esm && npm run build:es5",
    "preversion": "npm run lint && npm test && npm run build",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "bionode",
    "biojs",
    "genomics"
  ],
  "dependencies": {
    "@gmod/bgzf-filehandle": "^1.3.3",
    "abortable-promise-cache": "^1.4.1",
    "es6-promisify": "^6.0.1",
    "generic-filehandle": "^2.0.1",
    "long": "^4.0.0",
    "quick-lru": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/long": "^4.0.0",
    "@types/node": "^17.0.25",
    "@types/quick-lru": "2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
